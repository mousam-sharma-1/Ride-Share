<body>
  <h4 style="margin-left: 1%; color: rgb(85, 86, 87)"><%=name%></h4><br>
  <div class="table-responsive">
  
      <!-- <tr><td>Type</td> <td>Vehicle Type</td> <td>Seats</td> -->
        <!-- <td><h2>Source Address</h2></td><td><h2>Destination Address</h2></td> -->
  <% data.forEach(function(p){ %>
    <div style="box-shadow: 0 8px 8px 0 rgba(88, 87, 87, 0.2); border: 1px solid rgb(218, 213, 213);border-radius: 12px; max-width: 950px; margin-left: 1%;margin-right: 1%; overflow: hidden;">
      <table>
      <tr class="trr" style="background-color: rgba(133, 131, 131, 0.63)"><td style="padding: 1%"><%=p.type%></td><td style="padding: 2%"><%=p.Seat%></td><td><%=p.Seat%>Seat&nbsp</td></tr>
      <tr><td style="padding-left: 0.5%"><i class="fa fa-dot-circle-o" style="font-size:18px; color: rgb(54, 54, 223)"></i></td><td style="padding: 0.5%"><%=p.sor_address%></td></tr>
      <td style="padding-left: 1%"><i class="fa fa-ellipsis-v" style="font-size:18px; color: rgb(192, 189, 189)"></i><br></td>
      <tr><td style="padding-left: 0.5%"><i class="fa fa-map-marker" style="font-size:18px; color: rgb(248, 28, 28)"></i></td><td style="width: 100% ;padding: 0.5%"><%=p.des_address%></td></tr>
    </table>
  </div><br>
    <% }) %>

</div>
</body>
</html>